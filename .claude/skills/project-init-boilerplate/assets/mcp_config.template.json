{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {}
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@context7/mcp-server-context7"
      ],
      "env": {}
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "env": {}
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {}
    },
    "web-search-prime": {
      "command": "npx",
      "args": [
        "-y",
        "web-search-prime"
      ],
      "env": {}
    },
    "web-reader": {
      "command": "npx",
      "args": [
        "-y",
        "web-reader-mcp"
      ],
      "env": {}
    },
    "zread": {
      "command": "npx",
      "args": [
        "-y",
        "zread-mcp-server"
      ],
      "env": {}
    },
    "4.5v-image-analysis": {
      "command": "npx",
      "args": [
        "-y",
        "4.5v-mcp"
      ],
      "env": {}
    },
    "zai-mcp-server": {
      "command": "npx",
      "args": [
        "-y",
        "zai-mcp-server"
      ],
      "env": {}
    }
  }
}
